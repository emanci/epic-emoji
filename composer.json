{
    "name": "emanci/epic-emoji",
    "description": "The emoji epic conversion library",
    "keywords": ["emoji", "php-emoji"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{
        "name": "emanci",
        "email": "zhengchaopu@gmail.com"
    }],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "Emanci\\EpicEmoji\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
